{
  "name": "nomos-v5",
  "description": "Version 5 of nomos one web application",
  "author": "Ken Ha <ken.haduc@gmail.com> (http://github.com/khanhhaduc), David Bromley <dave@nomosone.com> (http://github.com/twophoney), Gareth Bedford <gareth.bedford@nomosone.com> (http://github.com/thirdman)",
  "license": "",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/NomosAdmin/nomos-v5-frontend"
  },
  "main": "bin/server.js",
  "scripts": {
    "dev-local": "concurrent --kill-others \"npm run watch-client-local\" \"npm run start-dev-local\"",
    "start-dev-local": "better-npm-run start-dev-local",
    "watch-client-local": "better-npm-run watch-client-local"
  },
  "betterScripts": {
    "start-dev-local": {
      "command": "node ./bin/server.js",
      "env": {
        "MODE": "local",
        "APP_HOST": "http://localhost",
        "API_HOST": "http://localhost",
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "watch-client-local": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "MODE": "local",
        "WEBPACK_HOST": "http://localhost",
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000,
        "APIPORT": 3030
      }
    }
  },
  "engines": {
    "node": "5.6.0"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "clean-webpack-plugin": "^0.1.10",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "http-proxy": "^1.14.0",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.15.0",
    "mobx": "^2.5.0",
    "mobx-react": "^3.5.5",
    "piping": "^1.0.0-rc.3",
    "pretty-error": "^2.0.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-helmet": "^3.1.0",
    "react-router": "^2.7.0",
    "react-router-scroll": "^0.3.2",
    "redbox-react": "^1.3.0",
    "safe-access": "^0.1.0",
    "scroll-behavior": "^0.8.1",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.3.0",
    "strip-loader": "^0.1.2",
    "superagent": "^2.2.0",
    "webpack-isomorphic-tools": "^2.5.8"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-typecheck": "^3.9.0",
    "better-npm-run": "0.0.11",
    "concurrently": "^2.2.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "lost": "^7.1.0",
    "node-sass": "^3.9.0",
    "postcss": "^5.1.2",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.8.0",
